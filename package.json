{
  "name": "@smolpack/eslint-config",
  "packageManager": "yarn@4.1.0",
  "version": "1.0.0",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "lint:shell": "node ./scripts/lint-shell.mjs",
    "lint": "node ./scripts/lint.mjs",
    "preflight": "node ./scripts/prerelease/preflight.mjs",
    "pack:check": "node ./scripts/prerelease/pack-check.mjs",
    "smoke:import": "node ./scripts/prerelease/smoke-import.mjs",
    "prerelease": "yarn preflight && yarn pack:check && yarn smoke:import",
    "prepublishOnly": "yarn prerelease"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "dependencies": {
    "typescript-eslint": "^8.51.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9.17.0",
    "eslint": ">= 9.17.0",
    "eslint-config-prettier": ">=9.1.0",
    "eslint-import-resolver-typescript": ">= 3.7.0",
    "eslint-plugin-import": ">= 2.31.0",
    "typescript": ">=5.7.2",
    "typescript-eslint": ">=8.18.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@smolpack/prettier-config": "^1.0.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}
